{
	"plugins": ["node_modules/jsdoc-babel", "plugins/markdown"],
	"babel": {
		"presets": ["env"]
	},
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": [
			"src/",
			"./README.md"
		]
	},
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true,
			"includeDate": false
		}
	},
	"docdash": {
		"static": false,
		"sort": true
	}
}
